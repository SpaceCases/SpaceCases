# .env.example

# Bot token for your application (from your bot service)
BOT_TOKEN=your-bot-token-here

# Database connection settings
DB_USER=your-db-username
DB_PASSWORD=your-db-password
DB_HOST=localhost
DB_PORT=5432
DB_NAME=your-database-name

# Optional: Test Guild ID for syncing slash commands (recommended during development)
TEST_GUILD=your-test-guild-id-here
